def lin(x, a, b):
    """returns ax+b"""
    return (a*x + b)

def quad(x, a, b, c):
    """returns ax^2 + bx + c"""
    return (a*(x**2) + b*x + c)

def cub(x,a,b,c,d):
    """returns ax^3 + bx^2 + cx + d"""
    return (a*(x**3) + b*(x**2) + c*x + d)

def expon(x, a, b, c):
    """returns ae^(-bx) + c"""
    return a * np.exp(-b * x) + c

