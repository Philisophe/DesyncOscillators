def running_mean2(x, N,N2=0):
    if N2==0:
        cumsum = np.cumsum(np.insert(x, 0, 0)) 
        return (cumsum[N:] - cumsum[:-N]) / float(N)
    else:
        cumsum = np.cumsum(np.insert(x, 0, 0)) 
        return running_mean(((cumsum[N:] - cumsum[:-N]) / float(N)),N,N2-1)
    
