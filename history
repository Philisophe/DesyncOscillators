def remdup(x):
    """Remove consecutive duplicates from the list"""
    i=0
    while i < len(x)-1:
        if x[i] == x[i+1]:
            del x[i]
        else:
            i = i+1
    return x


def npremdup(x):
    """Remove the consecutive duplicates from the np.array"""
    i=0
    while i < len(x)-1:
        if x[i] == x[i+1]:
            x = np.delete (x,i)
        else:
            i = i+1
    return x

