def model2 (x,y,t, A,alpha,omega,twist):
    dxdt = x*alpha*(A-np.sqrt(x**2 + y**2))-y*(omega + twist*(A-np.sqrt(x**2 + y**2)))
    dydt = y*alpha*(A-r)+x*(omega + twist*(A-r))
    dzdt=[dxdt, dydt]
    return dzdt

