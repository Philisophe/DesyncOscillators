def zero(x):
    zeroCross=[]
    for i in range(len(x[:,0]-1)):
        if (x[i,0]>0 and x[i+1,0]<=0):
            zeroCross.append((t[i]+t[i+1])/2) # we append to zeroCross the approx. time of 0-crossing
    return zeroCross

